PUT        /player/:name            controllers.LeagueController.addPlayer(name: String)
DELETE     /player/:id/:name        controllers.LeagueController.deletePlayer(id: String, name: String)
PUT        /game                    controllers.LeagueController.addGame
GET        /league/games            controllers.LeagueController.getAllGames
GET        /games/:id/:name         controllers.LeagueController.gamesForPlayer(id: String, name: String)
GET        /league/standings        controllers.LeagueController.getLeagueStandings
